generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  username  String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
model News {
  id          Int      @id @default(autoincrement())
  title       String
  imgUrl      String
  newsUrl     String
  date        DateTime @default(now())
}